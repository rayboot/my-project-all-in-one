<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="@style/Theme.Sherlock.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
            09-30 16:34:36.445: E/AndroidRuntime(8353): java.lang.RuntimeException: Unable to st
            art activity ComponentInfo{com.rayboot.hanzitingxie/com.rayboot.hanzitingxie.Ju
            mpActivity}: android.support.v4.app._HoloActivity$HoloThemeException: You must
             apply Holo.Theme, Holo.Theme.Light or Holo.Theme.Light.DarkActionBar the
             me on the activity (JumpActivity) for using HoloEverywhere
            
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        -->
        <item name="actionBarStyle">@style/Widget.Styled.ActionBar</item>
        <item name="android:actionBarStyle">@style/Widget.Styled.ActionBar</item>

        <item name="homeAsUpIndicator">@drawable/actionbar_indicator</item>
        <item name="android:homeAsUpIndicator" >@drawable/actionbar_indicator</item>
        <item name="android:actionMenuTextColor" >#ffffff</item>
        <item name="actionMenuTextColor" >#ffffff</item>
        <item name="android:windowAnimationStyle">@style/Animation.Activity</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="Animation.Activity" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/umeng_fb_slide_in_from_right</item>
        <item name="android:activityOpenExitAnimation">@anim/umeng_fb_slide_out_from_left</item>
        <item name="android:activityCloseEnterAnimation">@anim/umeng_fb_slide_in_from_left</item>
        <item name="android:activityCloseExitAnimation">@anim/umeng_fb_slide_out_from_right</item>

        <item name="android:taskOpenEnterAnimation">@anim/umeng_fb_slide_in_from_right</item>
        <item name="android:taskOpenExitAnimation">@anim/umeng_fb_slide_out_from_left</item>
        <item name="android:taskCloseEnterAnimation">@anim/umeng_fb_slide_in_from_left</item>
        <item name="android:taskCloseExitAnimation">@anim/umeng_fb_slide_out_from_right</item>

        <item name="android:taskToFrontEnterAnimation">@anim/umeng_fb_slide_in_from_right</item>
        <item name="android:taskToFrontExitAnimation">@anim/umeng_fb_slide_out_from_left</item>
        <item name="android:taskToBackEnterAnimation">@anim/umeng_fb_slide_in_from_left</item>
        <item name="android:taskToBackExitAnimation">@anim/umeng_fb_slide_out_from_right</item>

    </style>


    <style name="Widget.Styled.ActionBar" parent="Widget.Sherlock.Light.ActionBar.Solid.Inverse">
        <item name="background">#f97425</item>
        <item name="android:background">#f97425</item>

    </style>

    <style name="styleName">
        <item name="android:background">#ffffff</item>
        <item name="android:orientation">vertical</item>
    </style>
</resources>
